{
  "name": "wedding",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./public/dist/",
    "start": "./serve.sh",
    "build": "npm run build:script; npm run build:css",
    "build:script": "esbuild ./public/index.js --bundle --minify --outfile=./public/dist/index.min.js",
    "build:css": "postcss ./public/css/main.css --output ./public/dist/main.min.css",
    "dev": "npm run dev:script",
    "dev:script": "esbuild ./public/index.js --bundle --minify --sourcemap --outfile=./public/dist/index.min.js",
    "dev:css": "postcss ./public/css/main.css --output --map ./public/dist/main.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewgremlich/dna-wedding.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewgremlich/dna-wedding/issues"
  },
  "homepage": "https://github.com/andrewgremlich/dna-wedding#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "cssnano": "^4.1.10",
    "esbuild": "^0.8.49",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-nested": "^5.0.3",
    "postcss-preset-env": "^6.7.0"
  }
}
